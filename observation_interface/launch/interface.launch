<!-- Core Tools Interface -->
<!--
  Author: Ian Loefgren
  Last Modified: 7/3/2017

  The ROS launch file for the Core Tools observation interface, including a user
  interface, and image processing node.
-->

<launch>

  <!-- Interface -->
  <node name="obs_interface" pkg="observation_interface"
    type="interface.py" output="screen" />

  <!-- Cop Video Raw Image Processing -->
  <node name="image_processing" pkg="image_proc" type="image_proc"
    ns="/$(arg cop)/camera/rgb" />

</launch>
